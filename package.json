  "bugs": {
    "url": "https://github.com/gaoletian/nodejs-awesome/issues"
  },
  "homepage": "https://github.com/gaoletian/nodejs-awesome#readme",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-jest": "^19.0.0",
    "babel-preset-es2015": "^6.24.0",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "regenerator-runtime": "^0.10.3"
  },
  "dependencies": {
    "node-fetch": "^1.6.3",
    "nodemon": "^1.11.0"
  }
}
